<?xml version="1.0"?>
<package>
    <metadata>
        <id>libpjsua2maui</id>
        <title>libpjsua2maui</title>
        <version>2.15.1.1</version>
        <authors>Weslley Murdock</authors>
        <owners>Remote Code Hub</owners>
        <projectUrl>http://github.com/weslleymurdock/libpjsua2maui</projectUrl>
        <description>
            PJSIP bindings for .NET MAUI using version 2.15.1 with openh264, openssl, opus,
            and bcg729.
            The builds of native libraries targets iOS devices (arm64), iOS simulators (arm64, x86_64),
            android devices (arm64-v8a, armeabi-v7a) and android simulators (x86, x86_64).
        </description>
        <releaseNotes>
            - Fixed Page size warnings on android
            - Added arm64 simulators support on ios targets
        </releaseNotes>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">GPL-3.0-only</license>
        <copyright>Copyright Â©2025 Remote Code Hub</copyright>
        <tags>sip voip pjsip maui native</tags>
        <readme>README.md</readme>
        <iconUrl>https://raw.githubusercontent.com/weslleymurdock/libpjsua2maui/main/src/icon.png</iconUrl>
        <icon>icon.png</icon>
        <repository type="git" url="https://github.com/weslleymurdock/libpjsua2maui" />
        <references>
            <group targetFramework="net9.0-ios18.5">
                <reference file="pjsua2maui.ios.pdb" />
                <reference file="pjsua2maui.ios.dll" />
            </group>
            <group targetFramework="net9.0-android34.0">
                <reference file="pjsua2maui.android.pdb" />
                <reference file="pjsua2maui.android.dll" />
                <reference file="pjsua2maui.android.xml" />
            </group>
        </references>
        

        <dependencies>
            <group targetFramework="net9.0-ios18.5" />
            <group targetFramework="net9.0-android34.0" />
        </dependencies>
    </metadata>

    <files>
        <!-- Targets -->
        <file src="libpjsua2maui.targets" target="build\libpjsua2maui.targets" />

        <!-- Native iOS -->
        <file src="libpjsua2.a\lib\x86_64\libpjsua2.a"
            target="runtimes\iossimulator-x64\native\libpjsua2.a" />
        <file src="libpjsua2.a\lib\arm64s\libpjsua2.a"
            target="runtimes\iossimulator-arm64\native\libpjsua2.a" />
        <file src="libpjsua2.a\lib\arm64\libpjsua2.a"
            target="runtimes\ios-arm64\native\libpjsua2.a" />

        <!-- Native Android -->
        <!-- armeabi-v7a -->
        <file src="libpjsua2.so\lib\armeabi-v7a\*" target="runtimes\android-arm\native\" />
        <!-- arm64-v8a -->
        <file src="libpjsua2.so\lib\arm64-v8a\*" target="runtimes\android-arm64\native\" />
        <!-- x86 -->
        <file src="libpjsua2.so\lib\x86\*" target="runtimes\android-x86\native\" />
        <!-- x86_64 -->
        <file src="libpjsua2.so\lib\x86_64\*" target="runtimes\android-x86_64\native\" />

        <!-- Java bindings -->
        <file src="libpjsua2maui.android\org\pjsip\*.java" target="contentFiles\any\android\org\pjsip\" />

        <!-- Managed assemblies -->
        <file src="libpjsua2maui.ios\bin\Release\net9.0-ios\pjsua2maui.ios.*"
            target="lib\net9.0-ios18.5\" />
        <file src="libpjsua2maui.ios\bin\Release\net9.0-ios\pjsua2maui.ios.*"
            target="ref\net9.0-ios18.5\" />
        <file src="libpjsua2maui.android\bin\Release\net9.0-android\pjsua2maui.android.*"
            target="lib\net9.0-android34.0\" />
        <file src="libpjsua2maui.android\bin\Release\net9.0-android\pjsua2maui.android.*"
            target="ref\net9.0-android34.0\" />

        <!-- README -->
        <file src="README.md" target="." />
        <!-- ICON -->
        <file src="icon.png" target="." />
    </files>
</package>