<?xml version="1.0"?>
<package>
    <metadata>
        <id>libpjsua2maui</id>
        <title>libpjsua2maui</title>
        <version>2.15.1</version>
        <authors>Weslley Murdock</authors>
        <projectUrl>http://github.com/weslleymurdock/libpjsua2maui</projectUrl>
        <description>PJSIP bindings for .NET MAUI using version 2.15.1 with openh264, openssl, opus,
            and bcg729.</description>
        <releaseNotes>- Fixed linking issues on iOS\n- Upgraded PJSIP to 2.15.1\n- Unified
            third-party versions\n- Dropped support for Android lower than 9.0</releaseNotes>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">LGPL-3.0-or-later</license>
        <copyright>Copyright Â©2025 Remote Code Hub</copyright>
        <tags>sip voip pjsip maui native</tags>
        <readme>README.md</readme>

        <references>
            <group targetFramework="net9.0-ios18.5">
                <reference file="pjsua2.maui.ios.pdb" />
                <reference file="pjsua2.maui.ios.dll" />
            </group>
            <group targetFramework="net9.0-android34.0">
                <reference file="pjsua2maui.android.pdb" />
                <reference file="pjsua2maui.android.dll" />
                <reference file="pjsua2maui.android.xml" />
            </group>
        </references>


        <dependencies>
            <group targetFramework="net9.0-ios18.5" />
            <group targetFramework="net9.0-android34.0" />
        </dependencies>
    </metadata>

    <files>
        <!-- Targets -->
        <file src="libpjsua2maui.targets" target="build\libpjsua2maui.targets" />

        <!-- Native iOS -->
        <file src="libpjsua2maui.a\lib\x86_64\libpjsua2.a"
            target="runtimes\iossimulator-x64\native\libpjsua2.a" />
        <file src="libpjsua2maui.a\lib\arm64\libpjsua2.a"
            target="runtimes\ios-arm64\native\libpjsua2.a" />

        <!-- Native Android -->
        <!-- armeabi-v7a -->
        <file src="libpjsua2maui.so\lib\armeabi-v7a\*" target="runtimes\android-arm\native\" />
        <!-- arm64-v8a -->
        <file src="libpjsua2maui.so\lib\arm64-v8a\*" target="runtimes\android-arm64\native\" />
        <!-- x86 -->
        <file src="libpjsua2maui.so\lib\x86\*" target="runtimes\android-x86\native\" />
        <!-- x86_64 -->
        <file src="libpjsua2maui.so\lib\x86_64\*" target="runtimes\android-x86_64\native\" />

        <!-- Java bindings -->
        <file src="libpjsua2maui.so\org\pjsip\*.java" target="contentFiles\any\android\org\pjsip\" />

        <!-- Managed assemblies -->
        <file src="libpjsua2maui.ios\bin\Release\net9.0-ios\pjsua2.maui.ios.*"
            target="lib\net9.0-ios18.5\" />
        <file src="libpjsua2maui.android\bin\Release\net9.0-android\pjsua2maui.android.*"
            target="lib\net9.0-android34.0\" />
        <file src="libpjsua2maui.ios\bin\Release\net9.0-ios\pjsua2.maui.ios.*"
            target="ref\net9.0-ios18.5\" />
        <file src="libpjsua2maui.android\bin\Release\net9.0-android\pjsua2maui.android.*"
            target="ref\net9.0-android34.0\" />

        <!-- Readme -->
        <file src="README.md" target="" />
    </files>
</package>